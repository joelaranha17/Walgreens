<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.firstapp.joel.walgreens">
    <application>
        android:allowBackup="true"
        android:icon="@mipmap/walgreens"
        android:label="Walgreens"
        android:roundIcon="@mipmap/walgreens"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.firstapp.joel.walgreens.util.One.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".util.One.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        </activity>
        <activity android:name="com.firstapp.joel.walgreens.util.Login.LoginActivity" />
        <activity android:name="com.firstapp.joel.walgreens.util.Three.Third"/>
        <activity android:name="com.firstapp.joel.walgreens.util.Four.Fourth"/>
        <activity android:name="com.firstapp.joel.walgreens.util.Five.Fifth" />
        <activity android:name="com.firstapp.joel.walgreens.util.Login.Register"></activity>
    </application>

</manifest>